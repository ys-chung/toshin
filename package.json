{
  "name": "toshin",
  "version": "23.0.0",
  "license": "ISC",
  "author": "YS Chung <hello@yschung.org>",
  "type": "module",
  "engines": {
    "node": ">18.0"
  },
  "scripts": {
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "npm run lint && npm run typecheck && npm run start",
    "test": "ava"
  },
  "homepage": "https://github.com/ys-chung/toshin23#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ys-chung/toshin23.git"
  },
  "bugs": {
    "url": "https://github.com/ys-chung/toshin23/issues"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ],
    "files": [
      "test/**/*"
    ],
    "environmentVariables": {
      "SILENT": "true"
    }
  },
  "dependencies": {
    "@discordx/importer": "^1.1.10",
    "@scio/dice-typescript": "^1.6.2",
    "booru": "^2.6.3",
    "bufferutil": "^4.0.7",
    "discord.js": "^14.7.1",
    "discordx": "^11.7.1",
    "eiyuu": "^1.1.0",
    "erlpack": "github:discord/erlpack",
    "fetch-h2": "^3.0.2",
    "html-to-text": "^9.0.4",
    "pixiv.ts": "^0.5.9",
    "truncate": "^3.0.0",
    "twitter-api-v2": "^1.14.2",
    "utf-8-validate": "^6.0.3",
    "zlib-sync": "^0.1.8",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.39",
    "@swc/helpers": "^0.4.14",
    "@types/html-to-text": "^9.0.0",
    "@types/node": "^18.15.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "ava": "^5.2.0",
    "eslint": "^8.48.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-sonarjs": "^0.18.0",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
