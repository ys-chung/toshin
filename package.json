{
  "name": "toshin",
  "version": "1.0.0",
  "description": "Simple Discord bot",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "pnpm run lint && pnpm run build && pnpm run start",
    "lint": "eslint src",
    "fix": "eslint --fix src",
    "format": "prettier --write src",
    "build": "rm -rf ./dist && tsc",
    "start": "node dist/index.js",
    "preinstall": "npx only-allow pnpm"
  },
  "author": "YS Chung",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node16": "1.0.3",
    "@types/lodash": "4.14.182",
    "@types/node": "16.11.43",
    "@typescript-eslint/eslint-plugin": "5.30.3",
    "@typescript-eslint/parser": "5.30.3",
    "eslint": "8.19.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-import-resolver-typescript": "3.1.4",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "pixiv.ts": "0.5.7",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "booru": "^2.5.6",
    "discord.js": "^13.8.1",
    "fetch-h2": "^3.0.2",
    "lodash": "^4.17.21",
    "youtube-dl-exec": "^2.0.12"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
